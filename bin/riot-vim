#!/bin/bash
vim=$(xdotool getactivewindow)
xdotool search --name Riot windowactivate
while IFS= read -r p || [ -n "$p" ]
do
    printf '%s\n' "$p" | xclip -selection clipboard
    xdotool search --name Riot key ctrl+v
    sleep 0.05
done
sleep 0.05
xdotool search --name Riot key Return
xdotool windowactivate $vim
